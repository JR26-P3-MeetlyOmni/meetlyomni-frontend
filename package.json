    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-plugin-storybook": "^9.0.16",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "storybook": "^9.0.16",
    "typescript": "^5",
    "vitest": "^3.2.4"
